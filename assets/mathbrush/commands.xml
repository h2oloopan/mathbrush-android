<?xml version="1.0" encoding="utf-8"?>
<SCGLibCommands>
  <Command>
    <Name>Expand</Name>
    <ExpressionTypes>
      <Type>NULLTYPE</Type>
      <Type>POWER</Type>
      <Type>TIMES</Type>
      <Type>FRACTION</Type>
      <Type>PLUS</Type>
      <Type>FUNCTION</Type>
      <Type>EQUAL</Type>
    </ExpressionTypes>
    <CASCommands>
      <CASCommand>
        <CASType>MAPLE</CASType>
        <CASExpression>expand(%);</CASExpression>
        <NumberOfVariables>0</NumberOfVariables>
      </CASCommand>
      <CASCommand>
        <CASType>MATHEMATICA</CASType>
        <CASExpression>Expand[%];</CASExpression>
        <NumberOfVariables>0</NumberOfVariables>
      </CASCommand>
      <CASCommand>
        <CASType>SAGE</CASType>
        <CASExpression>casclient_expand(exprTree)</CASExpression>
        <NumberOfVariables>0</NumberOfVariables>
      </CASCommand>
    </CASCommands>
  </Command>
  <Command>
    <Name>Simplify</Name>
    <ExpressionTypes>
      <Type>NULLTYPE</Type>
      <Type>PLUS</Type>
      <Type>TIMES</Type>
      <Type>POWER</Type>
      <Type>FRACTION</Type>
    </ExpressionTypes>
    <CASCommands>
      <CASCommand>
        <CASType>MAPLE</CASType>
        <CASExpression>simplify(%);</CASExpression>
        <NumberOfVariables>0</NumberOfVariables>
      </CASCommand>
    </CASCommands>
   </Command>
  <Command>
    <Name>Evaluate</Name>
    <ExpressionTypes>
      <Type>NULLTYPE</Type>
      <Type>PLUS</Type>
      <Type>TIMES</Type>
      <Type>POWER</Type>
      <Type>FRACTION</Type>
      <Type>FUNCTION</Type>
    </ExpressionTypes>
    <CASCommands>
      <CASCommand>
        <CASType>MAPLE</CASType>
        <CASExpression>eval(%);</CASExpression>
        <NumberOfVariables>0</NumberOfVariables>
      </CASCommand>
    </CASCommands>
  </Command>
  <Command>
    <Name>Evaluate Numerically</Name>
    <ExpressionTypes>
      <Type>NULLTYPE</Type>
      <Type>PLUS</Type>
      <Type>TIMES</Type>
      <Type>POWER</Type>
      <Type>FRACTION</Type>
      <Type>FUNCTION</Type>
    </ExpressionTypes>
    <CASCommands>
      <CASCommand>
        <CASType>MAPLE</CASType>
        <CASExpression>evalf(%);</CASExpression>
        <NumberOfVariables>0</NumberOfVariables>
      </CASCommand>
    </CASCommands>
    <Conditions>
      <Condition>
        <FunctionName>HasVaribale</FunctionName>
        <ReturnType>bool</ReturnType>
        <ReturnValue>0</ReturnValue>
      </Condition>
    </Conditions>
  </Command>
  <Command>
    <Name>Import</Name>
    <CASCommands>
      <CASCommand>
        <CASType>MAPLE</CASType>
        <CASExpression>MathML[Import]("%1");</CASExpression>
        <NumberOfVariables>1</NumberOfVariables>
      </CASCommand>
    </CASCommands>
  </Command>
  <Command>
    <Name>Export</Name>
    <CASCommands>
      <CASCommand>
        <CASType>MAPLE</CASType>
        <CASExpression>MathML[ExportPresentation](%);</CASExpression>
        <NumberOfVariables>0</NumberOfVariables>
      </CASCommand>
    </CASCommands>
  </Command>
  <Command>
    <Name>Solve</Name>
    <NeedsVariables/>
    <ExpressionTypes>
      <Type>NULLTYPE</Type>
      <Type>EQUAL</Type>
      <Type>POWER</Type>
      <Type>FRACTION</Type>
      <Type>PLUS</Type>
      <Type>TIMES</Type>
    </ExpressionTypes>
    <CASCommands>
      <CASCommand>
        <CASType>MAPLE</CASType>
        <CASExpression>solve(%,%1);</CASExpression>
        <NumberOfVariables>1</NumberOfVariables>
      </CASCommand>
    </CASCommands>
    <Conditions>
      <Condition>
        <FunctionName>HasVaribale</FunctionName>
        <ReturnType>bool</ReturnType>
        <ReturnValue>1</ReturnValue>
      </Condition>
    </Conditions>
  </Command>
  <Command>
    <Name>Solve Set</Name>
      <ExpressionTypes>
        <Type>MULTIEXPR</Type>
      </ExpressionTypes>
  </Command>
  <Command>
    <Name>Factor</Name>
    <ExpressionTypes>
      <Type>PLUS</Type>
      <Type>POWER</Type>
      <Type>NUMERIC</Type>
      <Type>FRACTION</Type>
      <Type>TIMES</Type>
    </ExpressionTypes>
    <CASCommands>
      <CASCommand>
        <CASType>MAPLE</CASType>
        <CASExpression>factor(%);</CASExpression>
        <NumberOfVariables>0</NumberOfVariables>
      </CASCommand>
    </CASCommands>
  </Command>
  <Command>
    <Name>Determinant</Name>
    <ExpressionTypes>
      <Type>SQUAREMATRIX</Type>
    </ExpressionTypes>
    <CASCommands>
      <CASCommand>
        <CASType>MAPLE</CASType>
        <CASExpression>LinearAlgebra[Determinant](%);</CASExpression>
        <NumberOfVariables>0</NumberOfVariables>
      </CASCommand>
    </CASCommands>
  </Command>
  <Command>
    <Name>Inverse</Name>
    <ExpressionTypes>
      <Type>SQUAREMATRIX</Type>
    </ExpressionTypes>
    <CASCommands>
      <CASCommand>
        <CASType>MAPLE</CASType>
        <CASExpression>LinearAlgebra[MatrixInverse](%);</CASExpression>
        <NumberOfVariables>0</NumberOfVariables>
      </CASCommand>
    </CASCommands>
  </Command>
  <Command>
    <Name>Rank</Name>
    <ExpressionTypes>
      <Type>MATRIX</Type>
      <Type>SQUAREMATRIX</Type>
    </ExpressionTypes>
    <CASCommands>
      <CASCommand>
        <CASType>MAPLE</CASType>
        <CASExpression>LinearAlgebra[Rank](%);</CASExpression>
        <NumberOfVariables>0</NumberOfVariables>
      </CASCommand>
    </CASCommands>
  </Command>
  <Command>
    <Name>Null Space</Name>
    <ExpressionTypes>
      <Type>MATRIX</Type>
      <Type>SQUAREMATRIX</Type>
    </ExpressionTypes>
    <CASCommands>
      <CASCommand>
        <CASType>MAPLE</CASType>
        <CASExpression>LinearAlgebra[NullSpace](%);</CASExpression>
        <NumberOfVariables>0</NumberOfVariables>
      </CASCommand>
    </CASCommands>
  </Command>
</SCGLibCommands>